{
  "name": "git.interview",
  "version": "1.0.0",
  "description": "* Must use a Node and Express server",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --quiet .",
    "fix": "eslint --fix .",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --install --prefix client && npm run  build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MSU-developmentTeam/Git.Interview.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MSU-developmentTeam/Git.Interview/issues"
  },
  "homepage": "https://github.com/MSU-developmentTeam/Git.Interview#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "eslint": "^7.15.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.11.17",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
